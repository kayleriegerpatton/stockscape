<div id="create-portfolio-form-container" class="d-flex justify-content-center">
    <form id="portfolio-form" class="mx-4 my-5 p-3 rounded bg-light" style="min-width: 18rem;">
    <h2 class="mb-3">Create Portfolio</h2>
  <div class="mb-3">
    <label for="portfolio-title" class="form-label">Add a title</label>
    <input type="text" class="form-control" id="portfolio-title" />
    <div id="portfolio-error" class="form-text error"></div>
  </div>

  
  <button type="submit" class="btn btn-primary">Create</button>

</form>
  </div>
