<header>
  {{> private-navbar}}
</header>
<main>
  <div class="input-group flex-column d-flex align-items-center flex-wrap mx-4 mt-3 mb-5">
  <h1 class="h1 text-center mb-2" style="color: white;"><i class="fas fa-user"></i> Users</h1>
  <form class="d-flex" id="username-search-form">
        <input class="form-inline me-2" type="search" placeholder="Search" id="username" aria-label="Search">
        <button class="btn team-card btn-outline-light" type="submit" id="username-search">Search</button>
  </form>
</div>
  <div id="users-container" class="d-flex flex-wrap justify-content-around mx-4 mt-3 mb-5">
  {{#each user}}
  {{> user-card parent=../this}}
  {{/each}}
  </div>
</main>
<footer>{{> footer}}</footer>
