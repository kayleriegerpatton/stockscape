<div class="card m-3" style="width: 18rem;">
  <div class="card-body">
    <h3 class="card-title">{{name}}</h3>
    <h4>Returns: ${{user.score}}</h4>
    <h5 class="card-subtitle">Company Stocks</h5>
    {{#each companies}}
      <h6>{{name}}</h6>
      <p>{{symbol}}</p>
    {{/each}}
  </div>
</div>